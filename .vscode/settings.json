{
  "rust-analyzer.cargo.buildScripts.enable": true,
  "rust-analyzer.procMacro.enable": true,
  "files.watcherExclude": {
    "**/target": true
  },
  "rust-analyzer.files.excludeDirs": [
    "target"
  ],
  "rust-analyzer.checkOnSave.overrideCommand": [
    "cargo",
    "check",
    "-v",
    "--workspace",
    "--all-targets"
  ],
  "rust-analyzer.cargo.buildScripts.overrideCommand": [
    "cargo",
    "check",
    "-v",
    "--workspace",
    "--all-targets"
  ],
  "dart.analysisExcludedFolders": [
    "target"
  ],
  "yaml.schemas": {
    "https://raw.githubusercontent.com/AkihiroSuda/compose-go/8cf4f0bff89ac1e37dbe5a3bca2373ba4051871e/schema/compose-spec.json": [
      "docker-compose.*.yaml",
      "docker-compose.yaml"
    ],
  },
}
